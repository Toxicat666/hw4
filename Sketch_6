function setup() { 
  createCanvas(400, 400);
	background(0);
  fill(255);  
  noStroke();
  for(var y=25;y<400;y=y+25){
    var startX = y;
    var endX = 400-startX;
    if(y <=200){
    for(var x=startX;x<=endX;x=x+25){
      ellipse(x,y,25,25);
    }
  }else{
    for(var x = 400-y; x <= y; x = x+25){
      ellipse(x,y,25,25);
    }
  }
  
}
}
